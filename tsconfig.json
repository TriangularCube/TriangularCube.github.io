{
  "compilerOptions": {
    "module": "ES6",
    "target": "ESNext",
    "jsx": "preserve",
    "lib": ["DOM", "ESNext"],
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "moduleResolution": "Node"
  },
  "excludes": ["node_modules", "public", ".cache"]
}
